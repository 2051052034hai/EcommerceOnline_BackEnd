{
  "version": 2,
  "builds": [
      {
          "src": "src/server.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/server.js",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
      }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "src/server.js" }
],
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/ht-shop-three.vercel.app/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }  
]
  
}